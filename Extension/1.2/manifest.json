{
  "name": "Hide crap on Facebook",
  "description": "Removes videos and sponsored links from the Facebook timeline.",
  "version": "1.2",
  "manifest_version": 2,
   "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
	  "css": ["style.css"],
      "js": ["frameworks/jquery.js", "script.js"]
    }
  ],
  "permissions": [ 
    "storage"  
  ], 
  "options_page": "options.html",
  "icons": { "16": "images/icon16.png", "128": "images/icon128.png" }
} 